    "node-sass": "^4.14.1",
    "react": "^16.14.0",
    "react-burger-menu": "^2.9.2",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-loader-spinner": "^3.1.14",
    "react-masonry-css": "^1.0.14",
    "react-responsive": "^8.2.0",
    "react-responsive-carousel": "^3.2.11",
    "react-router-dom": "^5.2.0",
